{
  "name": "gauge-typescript",
  "version": "0.2.0",
  "description": "TypeScript language runner for gauge",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "build": "pnpm lint && tsc",
    "test:coverage": "jest --verbose --coverage --detectOpenHandles --forceExit",
    "codecov": "codecov",
    "test": "jest --verbose --detectOpenHandles --forceExit",
    "gen-proto": "sh genproto.sh",
    "prepublishOnly": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkitsunai/gauge-typescript.git"
  },
  "keywords": [
    "gauge",
    "Acceptance Test",
    "TypeScript",
    "Automation"
  ],
  "author": "BugDiver <vinayshankar00@gmail.com>",
  "contributors": [
    "tkitsunai <kitsunai.project@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tkitsunai/gauge-typescript/issues"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.3",
    "google-protobuf": "^3.21.2",
    "klaw-sync": "^6.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.4.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
    "@types/jest": "^29.5.12",
    "@types/klaw-sync": "^6.0.5",
    "@types/node": "^20.11.28",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "codecov": "^3.8.3",
    "eslint": "^8.57.0",
    "grpc-tools": "^1.12.4",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "jest": "^29.7.0",
    "jest-mock-process": "^2.0.0",
    "jest-ts-auto-mock": "^2.1.0",
    "prettier": "^3.2.5",
    "ts-auto-mock": "^3.7.2",
    "ts-jest": "^29.1.2",
    "ts-patch": "^3.1.2"
  }
